{
    "name": "nimbly/activeresource",
    "description": "Use a RESTful resource based API like a database",
    "keywords": [
        "rest",
        "api",
        "php",
        "model",
        "resource",
        "database",
        "activerecord",
        "active",
        "record"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Brent Scheffler",
            "email": "brent@brentscheffler.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ActiveResource\\": "src/"
        }
    },
    "require": {
        "php": "~5.4 || ~7.0",
        "guzzlehttp\/guzzle": "^6.2",
        "optimus\/onion": "^1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Tests\\Models\\": "tests/Models"
        }
    },
    "require-dev": {
        "phpunit\/phpunit": "^6.5"
    }
}