{
    "name": "nimbly/activeresource",
    "description": "Use a RESTful resource based API like a database",
    "keywords": [
        "rest",
        "api",
        "php",
        "model",
        "resource",
        "database",
        "activerecord",
        "active",
        "record"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Brent Scheffler",
            "email": "brent@brentscheffler.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ActiveResource\\": "src/"
        }
    },
    "require": {
        "php": ">=7.3|^8.0",
        "psr/http-client": "^1.0",
        "nimbly/shuttle": "^0.5.0",
        "nimbly/capsule": "^1.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit\/phpunit": "^9",
        "vimeo/psalm": "^4.11",
        "symfony/var-dumper": "^5.3"
    }
}
